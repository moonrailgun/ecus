{
  "name": "ecus",
  "version": "0.8.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cd app && pnpm dev",
    "release": "release-it",
    "release:patch": "release-it -i patch",
    "release:packages": "pnpm -r --filter \"./packages/*\" publish --registry=https://registry.npmjs.com/"
  },
  "pnpm": {
    "overrides": {
      "react": "18.2.0",
      "react-dom": "18.2.0"
    }
  },
  "packageManager": "pnpm@9.7.1",
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.0",
    "release-it": "^18.1.2"
  }
}
